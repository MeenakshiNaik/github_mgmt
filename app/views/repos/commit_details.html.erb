<div class = "container">
	<%= link_to "<span class='btn' ><i class='glyphicon glyphicon-chevron-left'></i></span>Back".html_safe ,repos_path(),class: "back btn btn-labeled btn-default " %><br/><br/>

	<span>
		<%= link_to "#{image_tag('GitHub-Mark-64px.png',:width => '25px')}#{@repo.github_repo_name.humanize}".html_safe,@repo.github_repo ,class: "github_link" %>
	</span>

	<div class="row border well">
		<div class="col-md-4" >
      <h4>List of Commits</h4>
        <div class = "commits list-group" data-name = "">        
        <%= @hook_commits["files"] %>
        </div>
    </div>
	</div>
</div>	

